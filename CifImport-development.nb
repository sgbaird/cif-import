                RowBox[{"#1", "[", 
                 RowBox[{"[", "1", "]"}], "]"}], "-", 
                RowBox[{"#2", "[", 
                 RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "<", ".0001"}], "&&", 
             RowBox[{
              RowBox[{"Abs", "[", 
               RowBox[{
                RowBox[{"#1", "[", 
                 RowBox[{"[", "2", "]"}], "]"}], "-", 
                RowBox[{"#2", "[", 
                 RowBox[{"[", "2", "]"}], "]"}]}], "]"}], "<", ".0001"}], "&&", 
